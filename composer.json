{
  "name": "sifophp/sifo",
  "description": "SIFO framework.",
  "type": "project",
  "keywords": [
    "php",
    "framework"
  ],
  "homepage": "https://github.com/sifophp/SIFO",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Albert Lombarte",
      "email": "alombarte@gmail.com",
      "role": "creator"
    }
  ],
  "support": {
    "issues": "https://github.com/sifophp/SIFO/issues"
  },
  "require": {
    "php": ">=7.0",
    "geoip/geoip": "^1.17",
    "neutron/sphinxsearch-api": "^2.0",
    "phpmailer/phpmailer": "^5.2",
    "sifophp/sifo-common-instance": "^2.12",
    "smarty/smarty": "3.1.24",
    "symfony/yaml": "^3.2",
    "tpyo/amazon-s3-php-class": "^0.5",
    "twig/extensions": "^1.4",
    "twig/twig": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "symfony/var-dumper": "^3.2"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
      "psr-4": {
          "Sifo\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Sifo\\": "test/"
      }
  }
}
